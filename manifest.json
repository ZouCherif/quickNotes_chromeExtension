{
  "manifest_version": 3,
  "name": "Bloc-notes Rapide",
  "version": "1.0",
  "description": "Une extension de bloc-notes rapide pour Chrome.",
  "permissions": ["activeTab", "storage", "notifications"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
